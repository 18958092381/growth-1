<!--pages/index/list/list.wxml-->
<view class="weui-cells">
  <block wx:for="{{list}}" wx:key="name">
    <view class="weui-cell" hover-class="weui-cell-active" bindtap="clickHandler" data-detail="{{item.timeline}}">
      <view class="weui-cell__bd">{{item.name||item.title}}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>
  </block>
</view>