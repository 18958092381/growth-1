<view class="page">
  <view class="page__bd">
    <view class="weui-cells__title">加入我们</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell">
        <view class="weui-cell__bd">Github</view>
        <view class="weui-cell__ft">https://github.com/phodal/growth</view>
      </view>
      <view class="weui-cell">
        <view class="weui-cell__bd">论坛</view>
        <view class="weui-cell__ft">http://forum.growth.ren</view>
      </view>
      <view class="weui-cell">
        <view class="weui-cell__bd">版本号</view>
        <view class="weui-cell__ft">1.0.0</view>
      </view>
    </view>
    <view class="weui-cells__title">团队成员</view>
    <view class="weui-cells weui-cells_after-title">
      <block wx:for="{{members}}"  wx:key="icon" wx:for-item="member">
        <view class="weui-cell">
          <view class="weui-cell__hd">
            <image src="{{member.icon}}" style=""></image>
          </view>
          <view class="weui-cell__bd flex-y">{{member.name}}</view>
        </view>
      </block>
    </view>
  </view>
</view>